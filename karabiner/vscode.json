{
  "title": "The setting for VSCode",
  "rules": [
    {
      "description": "From left_command to left_control in VSCode",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_command"
          },
          "to": {
            "key_code": "left_control"
          },
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.microsoft\\.VSCode",
                "^com\\.microsoft\\.VSCodeInsiders"
              ]
            }
          ]
        }
      ]
    }
  ]
}
